{
    "name": "pokedex",
    "version": "1.0.0",
    "main": "src/index.js",
    "scripts": {
        "start": "npm run dev",
        "dev": "webpack-dev-server --mode=development --open",
        "dev:backend": "cross-env BACKEND=PROD webpack-dev-server --mode=development --open",
        "dev:prod": "webpack-dev-server --mode=production --open",
        "build": "webpack --mode=production",
        "build:dev": "webpack --mode=development",
        "start:server": "node server.js",
        "format": "npm run format:js && npm run format:css",
        "format:js": "prettier --write \"src/**/*.{js,jsx}\"",
        "format:css": "prettier --write \"src/**/*.{css,less}\" && npm run format:styl",
        "format:styl": "stylus-supremacy format \"src/**/*.styl\" --replace --options .stylussupremacy",
        "lint": "eslint src --ext .js,.jsx"
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "npm run lint --fix",
            "prettier --write",
            "git add"
        ],
        "src/**/*.{css,less}": [
            "prettier --write",
            "git add"
        ],
        "src/**/*.styl": [
            "stylus-supremacy format --replace --options .stylussupremacy",
            "git add"
        ]
    },
    "dependencies": {
        "antd": "3.7.1",
        "classnames": "2.2.6",
        "decko": "1.2.0",
        "mobx": "5.9.0",
        "mobx-react": "5.4.3",
        "mobx-react-devtools": "6.0.3",
        "prop-types": "15.6.2",
        "react": "16.8",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "16.8",
        "react-router-dom": "4.3.1"
    },
    "devDependencies": {
        "@babel/core": "7.2.2",
        "@babel/plugin-proposal-class-properties": "7.2.3",
        "@babel/plugin-proposal-decorators": "7.2.3",
        "@babel/plugin-proposal-export-default-from": "7.2.0",
        "@babel/plugin-syntax-dynamic-import": "7.2.0",
        "@babel/plugin-syntax-import-meta": "7.2.0",
        "@babel/plugin-transform-runtime": "7.2.0",
        "@babel/preset-env": "7.2.3",
        "@babel/preset-react": "7.0.0",
        "@babel/runtime": "7.2.0",
        "@commitlint/cli": "7.3.2",
        "@commitlint/config-conventional": "7.3.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.1",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.5",
        "babel-plugin-import": "1.11.0",
        "clean-webpack-plugin": "1.0.0",
        "commitlint": "7.3.2",
        "compression-webpack-plugin": "^2.0.0",
        "copy-webpack-plugin": "4.6.0",
        "cross-env": "^5.2.0",
        "css-loader": "1.0.1",
        "cssnano": "4.1.8",
        "error-overlay-webpack-plugin": "0.1.6",
        "eslint": "5.12.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-config-prettier": "3.5.0",
        "eslint-config-standard": "^14.0.0",
        "eslint-import-resolver-webpack": "0.11.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "eslint-plugin-node": "^9.1.0",
        "eslint-plugin-prettier": "3.0.1",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "7.12.4",
        "eslint-plugin-standard": "^4.0.1",
        "express": "^4.17.0",
        "extract-text-webpack-plugin": "4.0.0-beta.0",
        "file-loader": "1.1.11",
        "html-webpack-plugin": "3.2.0",
        "less": "3.9.0",
        "less-loader": "4.1.0",
        "lint-staged": "8.1.0",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "postcss": "7.0.13",
        "postcss-loader": "2.1.6",
        "prettier": "1.13.4",
        "react-css-modules": "4.7.9",
        "replace": "1.0.1",
        "style-loader": "0.23.1",
        "stylus": "0.54.5",
        "stylus-loader": "3.0.2",
        "stylus-supremacy": "2.12.7",
        "svg-inline-loader": "0.8.0",
        "svg-inline-react": "3.1.0",
        "uglifyjs-webpack-plugin": "2.1.1",
        "webpack": "4.29.6",
        "webpack-cli": "3.3.0",
        "webpack-dev-server": "^3.2.1",
        "webpack-merge": "4.2.1"
    }
}
